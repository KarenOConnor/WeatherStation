        <TextBlock x:ConnectionId='2' x:Name="Humidity" HorizontalAlignment="Left" Margin="193,380,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="124" Width="577"/>
        <TextBlock x:ConnectionId='3' x:Name="Pressure" HorizontalAlignment="Left" Margin="193,504,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="124" Width="577"/>
    </Grid>
